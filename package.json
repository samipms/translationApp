{
    "name": "paint",
    "version": "7.0.2",
    "description": "Starter code for paint",
    "author": "Microsoft",
    "license": "MIT",
    "scripts": {
        "build:dev": "yarn run clean-dist && webpack --config=configs/webpack/dev.js",
        "build:prod": "yarn run clean-dist && webpack --config=configs/webpack/prod.js",
        "webversion": "webpack -v",
        "clean-dist": "rimraf dist/*",
        "lint": "eslint ./src/**/*.{js,ts,tsx} --quiet",
        "lint:fix": "eslint ./src/**/*.{js,ts,tsx} --fix",
        "start": "webpack serve --config=configs/webpack/dev.js",
        "start-prod": "webpack serve --config=configs/webpack/prod.js",
        "test": "jest --coverage --watchAll=false --config=configs/jest/jest.config.js",
        "test:coverage": "jest --passWithNoTests --coverage --watchAll=false --config=configs/jest.config.js",
        "refresh-vs-token": "vsts-npm-auth -config .npmrc",
        "lint-commit": "lint-staged",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@babel/plugin-transform-runtime": "^7.15.0",
        "@babel/preset-env": "^7.15.0",
        "@babel/preset-react": "^7.14.5",
        "@babel/preset-typescript": "^7.15.0",
        "@metaos/mfs-loader-dev": "0.1.30",
        "@microsoft/eslint-plugin-sdl": "^0.1.8",
        "@testing-library/jest-dom": "5.12.0",
        "@testing-library/react": "^12.1.2",
        "@testing-library/react-hooks": "^7.0.2",
        "@types/enzyme": "^3.10.9",
        "@types/jest": "^27.0.1",
        "@types/node": "^14.17.3",
        "@types/react": "^17.0.19",
        "@types/react-dom": "^17.0.9",
        "@types/w3c-image-capture": "^1.0.5",
        "@typescript-eslint/eslint-plugin": "^5.10.1",
        "@typescript-eslint/parser": "^5.10.1",
        "babel-loader": "^8.2.2",
        "camelcase": "^6.2.0",
        "cross-env": "^7.0.3",
        "crypto-browserify": "^3.12.0",
        "css-loader": "^6.2.0",
        "dotenv": "^8.2.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.6",
        "eslint": "^8.7.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-plugin-simple-import-sort": "^5.0.1",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.3.2",
        "husky": "^7.0.4",
        "identity-obj-proxy": "^3.0.0",
        "image-webpack-loader": "^8.1.0",
        "jest": "^27.0.6",
        "jest-canvas-mock": "^2.3.1",
        "jest-watch-typeahead": "^0.6.4",
        "lint-staged": "^12.3.2",
        "mini-css-extract-plugin": "^2.3.0",
        "postcss-loader": "^6.2.1",
        "prettier": "^2.3.2",
        "process": "^0.11.10",
        "querystring-es3": "^0.2.1",
        "react-app-polyfill": "^3.0.0",
        "react-hot-loader": "^4.13.0",
        "rimraf": "^3.0.2",
        "sass": "^1.49.0",
        "sass-loader": "^12.1.0",
        "stream-browserify": "^3.0.0",
        "style-loader": "^3.2.1",
        "tinylicious": "0.4.17169",
        "typescript": "^4.6.0-dev.20211201",
        "url": "^0.11.0",
        "webpack": "^5.60.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.0.0",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "@azure/msal-browser": "^2.16.0",
        "@fluentui/react": "^8.37.1",
        "@metaos/mfs-client": "^0.13.5",
        "@metaos/mfs-loader": "^0.13.1",
        "@microsoft/metaos-app-sdk": "0.1.0",
        "@microsoft/microsoft-graph-client": "^3.0.1",
        "@microsoft/microsoft-graph-types": "2.10.0",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "uuid": "^8.3.2"
    },
    "lint-staged": {
        "*.{tsx,ts,js,json}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*{.md,scss}": [
            "prettier --write"
        ]
    },
    "resolutions": {
        "url-parse": "1.5.10",
        "follow-redirects":"1.14.8",
        "engine.io":"4.0.0",
        "simple-get":"3.1.1",
        "semver-regex":"3.1.3",
        "isomorphic-git":"1.8.2",
        "redis":"3.1.1"
    }
}
